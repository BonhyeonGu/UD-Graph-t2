  </xs:complexType>
  <xs:complexType name="con:ElevationPropertyType">
    <xs:sequence>
      <xs:element ref="con:Elevation"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="con:FloorSurface" substitutionGroup="con:AbstractConstructionSurface" type="con:FloorSurfaceType">
    <xs:annotation>
      <xs:documentation>A FloorSurface is surface that represents the interior floor of a construction. An example is the floor of a room.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="con:FloorSurfaceType">
    <xs:complexContent>
      <xs:extension base="con:AbstractConstructionSurfaceType">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="con:adeOfFloorSurface" type="con:ADEOfFloorSurfacePropertyType">
