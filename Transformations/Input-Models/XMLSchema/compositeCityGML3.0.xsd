    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="con:FloorSurfacePropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="con:FloorSurface"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
    <xs:attributeGroup ref="gml:OwnershipAttributeGroup"/>
  </xs:complexType>
  <xs:element name="con:GroundSurface" substitutionGroup="con:AbstractConstructionSurface" type="con:GroundSurfaceType">
    <xs:annotation>
      <xs:documentation>A GroundSurface is a surface that represents the ground plate of a construction. The polygon defining the ground plate is congruent with the footprint of the construction.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="con:GroundSurfaceType">
    <xs:complexContent>
      <xs:extension base="con:AbstractConstructionSurfaceType">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="con:adeOfGroundSurface" type="con:ADEOfGroundSurfacePropertyType">
            <xs:annotation>
              <xs:documentation>Augments the GroundSurface with properties defined in an ADE.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
