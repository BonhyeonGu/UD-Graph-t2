  <xs:complexType name="con:GroundSurfacePropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="con:GroundSurface"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
    <xs:attributeGroup ref="gml:OwnershipAttributeGroup"/>
  </xs:complexType>
  <xs:element name="con:Height" substitutionGroup="gml:AbstractObject" type="con:HeightType">
    <xs:annotation>
      <xs:documentation>Height represents a vertical distance (measured or estimated) between a low reference and a high reference. [cf. INSPIRE]</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="con:HeightType">
    <xs:sequence>
      <xs:element name="con:highReference" type="gml:CodeType">
        <xs:annotation>
          <xs:documentation>Indicates the high point used to calculate the value of the height. [cf. INSPIRE]</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="con:lowReference" type="gml:CodeType">
        <xs:annotation>
          <xs:documentation>Indicates the low point used to calculate the value of the height. [cf. INSPIRE]</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="con:status" type="con:HeightStatusValueType">
        <xs:annotation>
          <xs:documentation>Indicates the way the height has been captured. [cf. INSPIRE]</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="con:value" type="gml:LengthType">
        <xs:annotation>
          <xs:documentation>Specifies the value of the height above or below ground. [cf. INSPIRE]</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="con:HeightPropertyType">
    <xs:sequence>
      <xs:element ref="con:Height"/>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="con:HeightStatusValueType">
    <xs:annotation>
      <xs:documentation>HeightStatusValue enumerates the different methods used to capture a height. [cf. INSPIRE]</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="estimated">
        <xs:annotation>
          <xs:documentation>Indicates that the height has been estimated and not measured. [cf. INSPIRE]</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="measured">
        <xs:annotation>
          <xs:documentation>Indicates that the height has been (directly or indirectly) measured. [cf. INSPIRE]</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="con:InteriorWallSurface" substitutionGroup="con:AbstractConstructionSurface" type="con:InteriorWallSurfaceType">
    <xs:annotation>
      <xs:documentation>An InteriorWallSurface is a surface that is visible from inside a construction. An example is the wall of a room.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="con:InteriorWallSurfaceType">
    <xs:complexContent>
      <xs:extension base="con:AbstractConstructionSurfaceType">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="con:adeOfInteriorWallSurface" type="con:ADEOfInteriorWallSurfacePropertyType">
            <xs:annotation>
              <xs:documentation>Augments the InteriorWallSurface with properties defined in an ADE.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="con:InteriorWallSurfacePropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="con:InteriorWallSurface"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
    <xs:attributeGroup ref="gml:OwnershipAttributeGroup"/>
  </xs:complexType>
