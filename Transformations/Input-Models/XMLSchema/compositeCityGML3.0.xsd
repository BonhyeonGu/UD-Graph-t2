  <xs:complexType name="con:DoorSurfacePropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="con:DoorSurface"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
    <xs:attributeGroup ref="gml:OwnershipAttributeGroup"/>
  </xs:complexType>
  <xs:element name="con:Elevation" substitutionGroup="gml:AbstractObject" type="con:ElevationType">
