  <xs:simpleType name="con:ConditionOfConstructionValueType">
    <xs:annotation>
      <xs:documentation>ConditionOfConstructionValue enumerates different conditions of a construction. [cf. INSPIRE]</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="declined">
        <xs:annotation>
          <xs:documentation>Indicates that the construction cannot be used under normal conditions, though its main elements (walls, roof) are still present. [cf. INSPIRE]</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="demolished">
        <xs:annotation>
          <xs:documentation>Indicates that the construction has been demolished. There are no more visible remains. [cf. INSPIRE]</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="functional">
        <xs:annotation>
          <xs:documentation>Indicates that the construction is functional. [cf. INSPIRE]</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="projected">
        <xs:annotation>
          <xs:documentation>Indicates that the construction is being designed. Construction works have not yet started. [cf. INSPIRE]</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ruin">
        <xs:annotation>
          <xs:documentation>Indicates that the construction has been partly demolished and some main elements (roof, walls) have been destroyed. There are some visible remains of the construction. [cf. INSPIRE]</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="underConstruction">
        <xs:annotation>
          <xs:documentation>Indicates that the construction is under construction and not yet functional. This applies only to the initial construction works of the construction and not to maintenance work. [cf. INSPIRE]</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="con:ConstructionEvent" substitutionGroup="gml:AbstractObject" type="con:ConstructionEventType">
    <xs:annotation>
      <xs:documentation>A ConstructionEvent is a data type used to describe a specific event that is associated with a construction. Examples are the issuing of a building permit or the renovation of a building.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="con:ConstructionEventType">
    <xs:sequence>
      <xs:element name="con:event" type="gml:CodeType">
        <xs:annotation>
          <xs:documentation>Indicates the specific event type.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="con:dateOfEvent" type="xs:date">
        <xs:annotation>
          <xs:documentation>Specifies the date at which the event took or will take place.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="con:description" type="xs:string">
        <xs:annotation>
          <xs:documentation>Provides additional information on the event.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="con:ConstructionEventPropertyType">
    <xs:sequence>
      <xs:element ref="con:ConstructionEvent"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="con:Door" substitutionGroup="con:AbstractFillingElement" type="con:DoorType">
    <xs:annotation>
      <xs:documentation>A Door is a construction for closing an opening intended primarily for access or egress or both. [cf. ISO 6707-1]</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="con:DoorType">
    <xs:complexContent>
      <xs:extension base="con:AbstractFillingElementType">
        <xs:sequence>
          <xs:element minOccurs="0" name="con:class" type="gml:CodeType">
            <xs:annotation>
              <xs:documentation>Indicates the specific type of the Door.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="con:function" type="gml:CodeType">
            <xs:annotation>
              <xs:documentation>Specifies the intended purposes of the Door.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="con:usage" type="gml:CodeType">
            <xs:annotation>
              <xs:documentation>Specifies the actual uses of the Door.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="con:address" type="core:AddressPropertyType">
            <xs:annotation>
              <xs:documentation>Relates to the addresses that are assigned to the Door.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="con:adeOfDoor" type="con:ADEOfDoorPropertyType">
            <xs:annotation>
              <xs:documentation>Augments the Door with properties defined in an ADE.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="con:DoorPropertyType">
    <xs:sequence minOccurs="0">
      <xs:element ref="con:Door"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
    <xs:attributeGroup ref="gml:OwnershipAttributeGroup"/>
  </xs:complexType>
  <xs:element name="con:DoorSurface" substitutionGroup="con:AbstractFillingSurface" type="con:DoorSurfaceType">
    <xs:annotation>
      <xs:documentation>A DoorSurface is either a boundary surface of a Door feature or a surface that seals an opening filled by a door.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="con:DoorSurfaceType">
    <xs:complexContent>
      <xs:extension base="con:AbstractFillingSurfaceType">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="con:address" type="core:AddressPropertyType">
            <xs:annotation>
              <xs:documentation>Relates to the addresses that are assigned to the DoorSurface.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="con:adeOfDoorSurface" type="con:ADEOfDoorSurfacePropertyType">
            <xs:annotation>
              <xs:documentation>Augments the DoorSurface with properties defined in an ADE.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
