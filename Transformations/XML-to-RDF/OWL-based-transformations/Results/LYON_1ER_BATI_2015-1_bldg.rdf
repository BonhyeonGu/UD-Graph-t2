@prefix bldg: <http://www.opengis.net/citygml/2.0/building#> .
@prefix core: <http://www.opengis.net/citygml/2.0/core#> .
@prefix data: <https://github.com/VCityTeam/UD-Graph/LYON_1ER_BATI_2015-1_bldg#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

data:CityModel_0 a core:CityModel,
        owl:NamedIndividual ;
    core:cityObjectMember data:Building_0 .

data:BuildingPart_0 a bldg:BuildingPart,
        owl:NamedIndividual ;
    bldg:boundedBy data:GroundSurface_0,
        data:RoofSurface_0,
        data:RoofSurface_1,
        data:RoofSurface_2,
        data:RoofSurface_3,
        data:RoofSurface_4,
        data:WallSurface_0,
        data:WallSurface_1,
        data:WallSurface_2,
        data:WallSurface_3,
        data:WallSurface_4,
        data:WallSurface_5,
        data:WallSurface_6,
        data:WallSurface_7 .

data:Building_0 a bldg:Building,
        owl:NamedIndividual ;
    bldg:consistsOfBuildingPart data:BuildingPart_0 .

data:GroundSurface_0 a bldg:GroundSurface,
        owl:NamedIndividual .

data:RoofSurface_0 a bldg:RoofSurface,
        owl:NamedIndividual .

data:RoofSurface_1 a bldg:RoofSurface,
        owl:NamedIndividual .

data:RoofSurface_2 a bldg:RoofSurface,
        owl:NamedIndividual .

data:RoofSurface_3 a bldg:RoofSurface,
        owl:NamedIndividual .

data:RoofSurface_4 a bldg:RoofSurface,
        owl:NamedIndividual .

data:WallSurface_0 a bldg:WallSurface,
        owl:NamedIndividual .

data:WallSurface_1 a bldg:WallSurface,
        owl:NamedIndividual .

data:WallSurface_2 a bldg:WallSurface,
        owl:NamedIndividual .

data:WallSurface_3 a bldg:WallSurface,
        owl:NamedIndividual .

data:WallSurface_4 a bldg:WallSurface,
        owl:NamedIndividual .

data:WallSurface_5 a bldg:WallSurface,
        owl:NamedIndividual .

data:WallSurface_6 a bldg:WallSurface,
        owl:NamedIndividual .

data:WallSurface_7 a bldg:WallSurface,
        owl:NamedIndividual .

