@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix app:   <http://www.opengis.net/citygml/appearance/2.0/appearance#> .
@prefix iso19136: <http://def.isotc211.org/iso19136/2007/GML> .
@prefix gsp:   <http://www.opengis.net/ont/geosparql#> .

app:Appearance      a owl:Class ;
    rdfs:subClassOf   gsp:Feature ;
    rdfs:subClassOf   iso19136:AbstractFeature .

app:AbstractSurfaceData a owl:Class ;
    rdfs:subClassOf       gsp:Feature ;
    rdfs:subClassOf       iso19136:AbstractFeature .
