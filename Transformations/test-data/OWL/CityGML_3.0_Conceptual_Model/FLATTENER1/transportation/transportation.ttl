tran:Track  a            owl:Class ;
        rdfs:label       "Track"@en ;
        rdfs:subClassOf  tran:AbstractTransportationSpace ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  tran:Intersection ;
                           owl:onProperty     tran:Track.intersection
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  tran:Section ;
                           owl:onProperty     tran:Track.section
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  owl:Class ;
                           owl:onProperty               tran:Track.class
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  owl:Class ;
                           owl:onProperty     tran:Track.function
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  owl:Class ;
                           owl:onProperty     tran:Track.usage
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  owl:Class ;
                           owl:onProperty     tran:Track.class
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  tran:ADEOfTrack ;
                           owl:onProperty     tran:Track.adeOfTrack
                         ] ;
        skos:definition  "A Track is a small path mainly used by pedestrians. Tracks can be segmented into Sections and Intersections."@en .

