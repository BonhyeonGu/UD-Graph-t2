                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  tran:ADEOfTrafficArea ;
                           owl:onProperty     tran:TrafficArea.adeOfTrafficArea
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  owl:Class ;
                           owl:onProperty     tran:TrafficArea.surfaceMaterial
                         ] ;
