@prefix core-owl: <http://www.opengis.net/citygml/3.0/core#> .
@prefix iso19150-2: <http://def.isotc211.org/iso19150/-2/2012/base#> .
@prefix iso19108-obj: <http://def.isotc211.org/iso19108/2006/TemporalObjects#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix doc:   <https://github.com/VCityTeam/UD-Graph/tree/master/Ontologies/Document/1.0//document#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix iso19103-2015-col: <http://def.isotc211.org/iso19103/2015/Collections#> .

doc:Tag  a               owl:Class ;
        rdfs:label       "Tag"@en ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:anyURI ;
                           owl:onProperty     doc:Tag.uri
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     doc:Tag.text
                         ] .

doc:Reference.referringTo
        a            owl:ObjectProperty ;
        rdfs:domain  doc:Reference ;
        rdfs:label   "referringTo"@en ;
        rdfs:range   core-owl:AbstractTopLevelCityObject .

doc:Reference.referringDate
        a            owl:ObjectProperty ;
        rdfs:domain  doc:Reference ;
        rdfs:label   "referringDate"@en ;
        rdfs:range   iso19108-obj:TM_Instant .

doc:Reference.coveragePeriod
        a            owl:ObjectProperty ;
        rdfs:domain  doc:Reference ;
        rdfs:label   "coveragePeriod"@en ;
        rdfs:range   iso19108-obj:TM_Period .

doc:AbstractDocument.uri
        a            owl:DatatypeProperty ;
        rdfs:domain  doc:AbstractDocument ;
        rdfs:label   "uri"@en ;
        rdfs:range   xsd:anyURI .

doc:AbstractDocument  a  owl:Class ;
        rdfs:label       "AbstractDocument"@en ;
        rdfs:subClassOf  core-owl:AbstractTopLevelCityObject , core-owl:AbstractFeatureWithLifespan ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  doc:Tag ;
                           owl:onProperty     doc:AbstractDocument.tag
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  doc:Reference ;
                           owl:onProperty     doc:AbstractDocument.referringTo
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:anyURI ;
                           owl:onProperty     doc:AbstractDocument.uri
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     doc:AbstractDocument.title
                         ] .

doc:AbstractDocument.title
        a            owl:DatatypeProperty ;
        rdfs:domain  doc:AbstractDocument ;
        rdfs:label   "title"@en ;
        rdfs:range   xsd:string .

doc:Tag.text  a      owl:DatatypeProperty ;
        rdfs:domain  doc:Tag ;
        rdfs:label   "text"@en ;
        rdfs:range   xsd:string .

doc:AbstractDocument.tag
        a            owl:ObjectProperty ;
        rdfs:domain  doc:AbstractDocument ;
        rdfs:label   "tag"@en ;
        rdfs:range   doc:Tag .

doc:Reference  a         owl:Class ;
        rdfs:label       "Reference"@en ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  core-owl:AbstractTopLevelCityObject ;
                           owl:onProperty     doc:Reference.referringTo
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  iso19108-obj:TM_Instant ;
                           owl:onProperty     doc:Reference.referringDate
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  iso19103-2015-col:Code ;
                           owl:onProperty     doc:Reference.purposeType
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     doc:Reference.purpose
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  iso19108-obj:TM_Period ;
                           owl:onProperty     doc:Reference.coveragePeriod
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     doc:Reference.referenceText
                         ] .

doc:AbstractDocument.referringTo
        a            owl:ObjectProperty ;
        rdfs:domain  doc:AbstractDocument ;
        rdfs:label   "referringTo"@en ;
        rdfs:range   doc:Reference .

doc:Tag.uri  a       owl:DatatypeProperty ;
        rdfs:domain  doc:Tag ;
        rdfs:label   "uri"@en ;
        rdfs:range   xsd:anyURI .

doc:Reference.referenceText
        a            owl:DatatypeProperty ;
        rdfs:domain  doc:Reference ;
        rdfs:label   "referenceText"@en ;
        rdfs:range   xsd:string .

<https://github.com/VCityTeam/UD-Graph/tree/master/Ontologies/Document/1.0//document>
        a            owl:Ontology ;
        rdfs:label   "Document"@en ;
        owl:imports  <http://def.isotc211.org/iso19150/-2/2012/base> .

doc:Reference.purpose
        a            owl:DatatypeProperty ;
        rdfs:domain  doc:Reference ;
        rdfs:label   "purpose"@en ;
        rdfs:range   xsd:string .

doc:Reference.purposeType
        a            owl:ObjectProperty ;
        rdfs:domain  doc:Reference ;
        rdfs:label   "purposeType"@en ;
        rdfs:range   iso19103-2015-col:Code .
