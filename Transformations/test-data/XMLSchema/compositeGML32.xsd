	<!-- =========================================================== -->
	<xs:complexType name="TopologyStylePropertyType">
		<xs:annotation>
			<xs:documentation/>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="gml:TopologyStyle" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="about" type="xs:anyURI" use="optional"/>
		<xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
	</xs:complexType>
	<!-- ==============================================================
       Topology Style
	============================================================== -->
	<xs:element name="TopologyStyle" type="gml:TopologyStyleType" substitutionGroup="gml:AbstractGML">
		<xs:annotation>
			<xs:documentation>The style descriptor for topologies of a feature. Describes individual topology elements styles.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<!-- =========================================================== -->
	<xs:complexType name="TopologyStyleType">
		<xs:annotation>
			<xs:documentation>[complexType of] The style descriptor for topologies of a feature. Describes individual topology elements styles.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="gml:BaseStyleDescriptorType">
				<xs:sequence>
					<xs:choice>
						<xs:element ref="gml:symbol"/>
						<xs:element name="style" type="xs:string">
							<xs:annotation>
								<xs:appinfo>deprecated</xs:appinfo>
								<xs:documentation>Deprecated in GML version 3.1.0. Use symbol with inline content instead.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:element ref="gml:labelStyle" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="topologyProperty" type="xs:string"/>
				<xs:attribute name="topologyType" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ==============================================================
       Label Style Property
	============================================================== -->
