        skos:prefLabel   "value"@en .

con:Window.usage  a      owl:ObjectProperty ;
        rdfs:domain      con:Window ;
        rdfs:range       con:WindowUsageValue ;
        skos:definition  "Specifies the actual uses of the Window."@en ;
        skos:prefLabel   "usage"@en .

con:ADEOfOuterFloorSurface
        a                      owl:Class ;
        iso19150-2:isAbstract  true ;
