        skos:definition  "Augments AbstractFurniture with properties defined in an ADE."@en ;
        skos:prefLabel   "adeOfAbstractFurniture"@en .

con:Height  a            owl:Class ;
        skos:definition  "Height represents a vertical distance (measured or estimated) between a low reference and a high reference. [cf. INSPIRE]"@en ;
        skos:prefLabel   "Height"@en .

con:OtherConstruction.class
        a                owl:ObjectProperty ;
        rdfs:domain      con:OtherConstruction ;
        rdfs:range       con:OtherConstructionClassValue ;
        skos:definition  "Indicates the specific type of the OtherConstruction."@en ;
        skos:prefLabel   "class"@en .

con:EventValue  a        owl:Class ;
        rdfs:subClassOf  skos:Concept ;
        skos:definition  "EventValue is a code list that enumerates the different events of a construction."@en ;
        skos:prefLabel   "EventValue"@en .

