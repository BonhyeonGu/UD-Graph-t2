@prefix dct:   <http://purl.org/dc/terms/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix lusecode: <https://github.com/VCityTeam/UD-Graph/Ontologies/landuse/code#> .

<https://github.com/VCityTeam/UD-Graph/Ontologies/landuse/code>
        a       owl:Ontology .

lusecode:LandUseClassValue
        a                skos:ConceptScheme ;
        dct:isFormatOf   <https://github.com/VCityTeam/UD-Graph/Ontologies/landuse#LandUseClassValue> ;
        skos:definition  "LandUseClassValue is a code list used to further classify a LandUse."@en ;
        skos:prefLabel   "LandUseClassValue"@en .

lusecode:LandUseUsageValue
        a                skos:ConceptScheme ;
        dct:isFormatOf   <https://github.com/VCityTeam/UD-Graph/Ontologies/landuse#LandUseUsageValue> ;
        skos:definition  "LandUseUsageValue is a code list that enumerates the different uses of a LandUse."@en ;
        skos:prefLabel   "LandUseUsageValue"@en .

lusecode:LandUseFunctionValue
        a                skos:ConceptScheme ;
        dct:isFormatOf   <https://github.com/VCityTeam/UD-Graph/Ontologies/landuse#LandUseFunctionValue> ;
        skos:definition  "LandUseFunctionValue is a code list that enumerates the different purposes of a LandUse."@en ;
        skos:prefLabel   "LandUseFunctionValue"@en .
