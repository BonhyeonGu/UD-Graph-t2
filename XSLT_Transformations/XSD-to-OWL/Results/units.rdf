   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#derivationUnitTerm">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#DerivationUnitTermType"/>
      <rdfs:comment>Warning: This class type was declared outside of its original schema. This class may be declared incorrectly.</rdfs:comment>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#conversionToPreferredUnit">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#ConversionToPreferredUnitType"/>
      <rdfs:comment>Warning: This class type was declared outside of its original schema. This class may be declared incorrectly.</rdfs:comment>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#roughConversionToPreferredUnit">
