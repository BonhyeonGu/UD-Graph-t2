   </owl:Class>
   <owl:ObjectProperty rdf:about="http://www.opengis.net/gml#hasquantityType">
      <rdfs:comment>Warning: This ObjectProperty reference was declared outside of its original schema. This ObjectProperty may be declared incorrectly.</rdfs:comment>
      <rdfs:domain rdf:resource="http://www.opengis.net/gml#UnitDefinitionType"/>
      <rdfs:range rdf:resource="http://www.opengis.net/gml#quantityType"/>
   </owl:ObjectProperty>
   <owl:ObjectProperty rdf:about="http://www.opengis.net/gml#hascatalogSymbol">
