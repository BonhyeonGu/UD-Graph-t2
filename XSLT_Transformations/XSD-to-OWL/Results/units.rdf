   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#DerivedUnit">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#UnitDefinition"/>
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#DerivedUnitType"/>
      <rdfs:comment>Warning: This class type was declared outside of its original schema. This class may be declared incorrectly.</rdfs:comment>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#ConventionalUnit">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#UnitDefinition"/>
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#ConventionalUnitType"/>
      <rdfs:comment>Warning: This class type was declared outside of its original schema. This class may be declared incorrectly.</rdfs:comment>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#quantityType">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#StringOrRefType"/>
      <rdfs:comment>Warning: This class type was declared outside of its original schema. This class may be declared incorrectly.</rdfs:comment>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#catalogSymbol">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#CodeType"/>
