   <owl:Class rdf:about="http://www.opengis.net/gml#TimePeriodType">
      <rdfs:subClassOf>
         <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection"/>
         </owl:Class>
      </rdfs:subClassOf>
      <owl:disjointUnionOf rdf:parseType="Collection">
         <owl:Restriction>
            <owl:onProperty rdf:resource="http://www.opengis.net/gml#beginPosition"/>
