   <rdfs:Datatype rdf:about="http://www.opengis.net/gml#TimeUnitType">
      <rdfs:comment xml:lang="en">Standard units for measuring time intervals (see ISO 31-1).</rdfs:comment>
      <owl:equivalentClass>
         <rdfs:Datatype>
            <owl:oneOf>
               <rdf:Description rdf:type="http://www.w3.org/1999/02/22-rdf-syntax-ns#List">
                  <rdf:first rdf:datatype="string">year</rdf:first>
                  <rdf:rest>
                     <rdf:Description rdf:type="http://www.w3.org/1999/02/22-rdf-syntax-ns#List">
                        <rdf:first rdf:datatype="string">day</rdf:first>
                        <rdf:rest>
                           <rdf:Description rdf:type="http://www.w3.org/1999/02/22-rdf-syntax-ns#List">
                              <rdf:first rdf:datatype="string">hour</rdf:first>
                              <rdf:rest>
                                 <rdf:Description rdf:type="http://www.w3.org/1999/02/22-rdf-syntax-ns#List">
                                    <rdf:first rdf:datatype="string">minute</rdf:first>
                                    <rdf:rest>
                                       <rdf:Description rdf:type="http://www.w3.org/1999/02/22-rdf-syntax-ns#List">
                                          <rdf:first rdf:datatype="string">second</rdf:first>
                                          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
