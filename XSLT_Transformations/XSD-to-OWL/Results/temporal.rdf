               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#has_TimeGeometricPrimitive"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#_TimeGeometricPrimitive"/>
               </owl:Restriction>
            </owl:intersectionOf>
         </owl:Class>
      </rdfs:subClassOf>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#TimeInstantType">
      <rdfs:comment>Omit back-pointers begunBy, endedBy.</rdfs:comment>
      <rdfs:subClassOf>
         <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#hastimePosition"/>
