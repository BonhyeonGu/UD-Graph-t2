   <owl:Class rdf:about="http://www.opengis.net/gml#AbstractTimeObjectType">
      <rdfs:comment>This is an abstract class</rdfs:comment>
      <rdfs:comment xml:lang="en">The abstract supertype for temporal objects.</rdfs:comment>
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#AbstractGMLType"/>
      <rdfs:comment>Warning: The parent class 'gml:AbstractGMLType' is declared outside of this class' original schema. This class may be declared incorrectly.</rdfs:comment>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#AbstractTimePrimitiveType">
      <rdfs:comment>This is an abstract class</rdfs:comment>
      <rdfs:comment xml:lang="en">The abstract supertype for temporal primitives.</rdfs:comment>
      <rdfs:subClassOf>
         <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#relatedTime"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#RelatedTimeType"/>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#relatedTime"/>
                  <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
               </owl:Restriction>
            </owl:intersectionOf>
         </owl:Class>
      </rdfs:subClassOf>
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#AbstractTimeObjectType"/>
      <rdfs:comment>Warning: The parent class 'gml:AbstractTimeObjectType' is declared outside of this class' original schema. This class may be declared incorrectly.</rdfs:comment>
   </owl:Class>
