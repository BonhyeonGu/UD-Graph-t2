   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#target">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#TargetPropertyType"/>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#subject">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#target"/>
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#TargetPropertyType"/>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#resultOf">
      <rdfs:subClassOf rdf:resource="AssociationType"/>
      <rdfs:comment>Warning: This class type was declared outside of its original schema. This class may be declared incorrectly.</rdfs:comment>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#Observation">
