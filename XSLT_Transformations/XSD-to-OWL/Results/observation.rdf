   <owl:Class rdf:about="http://www.opengis.net/gml#TargetPropertyType">
      <rdfs:comment>Container for an object representing the target or subject of an observation.</rdfs:comment>
      <owl:disjointUnionOf rdf:parseType="Collection">
         <owl:Restriction>
            <owl:onProperty rdf:resource="http://www.opengis.net/gml#has_Feature"/>
            <owl:someValuesFrom rdf:resource="http://www.opengis.net/gml#_Feature"/>
         </owl:Restriction>
         <owl:Restriction>
            <owl:onProperty rdf:resource="http://www.opengis.net/gml#has_Geometry"/>
            <owl:someValuesFrom rdf:resource="http://www.opengis.net/gml#_Geometry"/>
         </owl:Restriction>
      </owl:disjointUnionOf>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#ObservationType">
      <rdfs:subClassOf>
         <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#hasvalidTime"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#validTime"/>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#hasusing"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#using"/>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#hasusing"/>
                  <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#hastarget"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#target"/>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#hastarget"/>
                  <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#hasresultOf"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#resultOf"/>
               </owl:Restriction>
            </owl:intersectionOf>
         </owl:Class>
      </rdfs:subClassOf>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#DirectedObservationType">
      <rdfs:subClassOf>
         <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#hasdirection"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#direction"/>
               </owl:Restriction>
            </owl:intersectionOf>
         </owl:Class>
      </rdfs:subClassOf>
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#ObservationType"/>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#DirectedObservationAtDistanceType">
      <rdfs:subClassOf>
         <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#distance"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#MeasureType"/>
               </owl:Restriction>
            </owl:intersectionOf>
         </owl:Class>
      </rdfs:subClassOf>
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#DirectedObservationType"/>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#using">
