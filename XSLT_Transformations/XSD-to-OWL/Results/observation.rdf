      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#ObservationType"/>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#DirectedObservation">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#Observation"/>
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#DirectedObservationType"/>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#DirectedObservationAtDistance">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#DirectedObservation"/>
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#DirectedObservationAtDistanceType"/>
   </owl:Class>
   <owl:ObjectProperty rdf:about="http://www.opengis.net/gml#has_Feature">
