               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="gml:hasdescription"/>
                  <owl:allValuesFrom rdf:resource="gml:description"/>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="gml:hasdescription"/>
                  <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="gml:hasname"/>
