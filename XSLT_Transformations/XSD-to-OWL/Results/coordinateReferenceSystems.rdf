      <rdfs:comment>Warning: This class type was declared outside of its original schema. This class may be declared incorrectly.</rdfs:comment>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#verticalCRSRef">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#VerticalCRSRefType"/>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#GeocentricCRS">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#_CoordinateReferenceSystem"/>
