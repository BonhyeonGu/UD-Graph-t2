   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#compoundCRSRef">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#CompoundCRSRefType"/>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#GeographicCRS">
