   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#geographicCRSRef">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#GeographicCRSRefType"/>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#VerticalCRS">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#_CoordinateReferenceSystem"/>
