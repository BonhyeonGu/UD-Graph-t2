   <owl:Class rdf:about="http://www.opengis.net/gml#coordinateReferenceSystemRef">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#CoordinateReferenceSystemRefType"/>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#CompoundCRS">
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#_CRS"/>
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#CompoundCRSType"/>
   </owl:Class>
