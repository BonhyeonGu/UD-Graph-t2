   <owl:Class rdf:about="http://www.opengis.net/gml#AbstractTimeSliceType">
      <rdfs:comment>This is an abstract class</rdfs:comment>
      <rdfs:comment xml:lang="en">A timeslice encapsulates the time-varying properties of a dynamic feature--it 
        must be extended to represent a timestamped projection of a feature. The dataSource 
        property describes how the temporal data was acquired.</rdfs:comment>
      <rdfs:subClassOf>
         <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#hasvalidTime"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#validTime"/>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#hasdataSource"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#dataSource"/>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#hasdataSource"/>
                  <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
               </owl:Restriction>
            </owl:intersectionOf>
         </owl:Class>
      </rdfs:subClassOf>
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#AbstractGMLType"/>
      <rdfs:comment>Warning: The parent class 'gml:AbstractGMLType' is declared outside of this class' original schema. This class may be declared incorrectly.</rdfs:comment>
   </owl:Class>
   <owl:Class rdf:about="http://www.opengis.net/gml#MovingObjectStatusType">
      <rdfs:comment xml:lang="en">This type encapsulates various dynamic properties of moving objects        
             (points, lines, regions). It is useful for dealing with features whose        
             geometry or topology changes over time.</rdfs:comment>
      <rdfs:subClassOf>
         <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#haslocation"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#location"/>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#speed"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#MeasureType"/>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#speed"/>
                  <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#bearing"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#DirectionPropertyType"/>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#bearing"/>
                  <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#acceleration"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#MeasureType"/>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#acceleration"/>
                  <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#elevation"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#MeasureType"/>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#elevation"/>
                  <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#hasstatus"/>
                  <owl:allValuesFrom rdf:resource="http://www.opengis.net/gml#status"/>
               </owl:Restriction>
               <owl:Restriction>
                  <owl:onProperty rdf:resource="http://www.opengis.net/gml#hasstatus"/>
                  <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
               </owl:Restriction>
            </owl:intersectionOf>
         </owl:Class>
      </rdfs:subClassOf>
      <rdfs:subClassOf rdf:resource="http://www.opengis.net/gml#AbstractTimeSliceType"/>
      <rdfs:comment>Warning: The parent class 'gml:AbstractTimeSliceType' is declared outside of this class' original schema. This class may be declared incorrectly.</rdfs:comment>
   </owl:Class>
