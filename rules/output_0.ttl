type:delete a owl:NamedIndividual,
        vers:TransactionTypeValue ;
    rdfs:label "delete"@en ;
    ns1:definition "An old feature is removed from the version"@en ;
    ns1:inScheme <https://raw.githubusercontent.com/VCityTeam/UD-Graph/master/Ontologies/Workspace/3.0/transactiontype> .

type:fused a owl:NamedIndividual,
        vers:TransactionTypeValue ;
    rdfs:label "fused"@en ;
    ns1:definition "Reverse_correspondence_size_length > 1... It can be: a split building that didn't work for both timestamps and which then denotes a logical fusion; a fusion or a split building problem + modifications; a split building problems + modifications"@en ;
    ns1:inScheme <https://raw.githubusercontent.com/VCityTeam/UD-Graph/master/Ontologies/Workspace/3.0/transactiontype> .

type:insert a owl:NamedIndividual,
        vers:TransactionTypeValue ;
    rdfs:label "insert"@en ;
    ns1:definition "A new feature is added to the version."@en ;
    ns1:inScheme <https://raw.githubusercontent.com/VCityTeam/UD-Graph/master/Ontologies/Workspace/3.0/transactiontype> .

type:modified a owl:NamedIndividual,
        vers:TransactionTypeValue ;
    rdfs:label "modified"@en ;
    ns1:definition "The building geometry and ID have changed e.g. it was heightened; its footprint geometry changed, etc.."@en ;
    ns1:inScheme <https://raw.githubusercontent.com/VCityTeam/UD-Graph/master/Ontologies/Workspace/3.0/transactiontype> .

type:re-ided a owl:NamedIndividual,
        vers:TransactionTypeValue ;
    rdfs:label "re-ided"@en ;
    ns1:definition "The building has been re-ided: same geometry, different (gml) ID."@en ;
    ns1:inScheme <https://raw.githubusercontent.com/VCityTeam/UD-Graph/master/Ontologies/Workspace/3.0/transactiontype> .

type:subdivided a owl:NamedIndividual,
        vers:TransactionTypeValue ;
    rdfs:label "subdivided"@en ;
    ns1:definition "Current building has more than one correspondent."@en ;
    ns1:inScheme <https://raw.githubusercontent.com/VCityTeam/UD-Graph/master/Ontologies/Workspace/3.0/transactiontype> .

type:unchanged a owl:NamedIndividual,
        vers:TransactionTypeValue ;
    rdfs:label "unchanged"@en ;
    ns1:definition "Unchanged building: same geometry, same (gml) ID."@en ;
    ns1:inScheme <https://raw.githubusercontent.com/VCityTeam/UD-Graph/master/Ontologies/Workspace/3.0/transactiontype> .

